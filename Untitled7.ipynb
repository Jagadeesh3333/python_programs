    "n = int(input())\n",
    "li = list(map(int, input().split()))\n",
    "\n",
    "evensum = 0\n",
    "oddsum = 0\n",
    "\n",
    "for i in range(n):\n",
    "    if li[i] % 2 == 0:\n",
    "        evensum += li[i]\n",
    "    else:\n",
    "        oddsum += li[i]\n",
    "        \n",
    "print(evensum,oddsum)"
